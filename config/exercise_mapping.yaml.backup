# Exercise to Muscle Group Mapping Configuration
version: "2.0"

# Muscle group hierarchy (3 levels)
muscle_groups:
  level1:
    - upper
    - lower
    - full_body
    
  level2:
    upper: [push, pull]
    lower: [legs]
    full_body: [compound]
    
  level3:
    push: [chest, shoulders, triceps]
    pull: [back, biceps, forearms]
    legs: [quads, hamstrings, glutes, calves, abs, obliques, lower_back]
    compound: [full_body]

# Exercise mappings (case-insensitive matching)
exercises:
