# Fuzzy matching rules for unmapped exercises
fuzzy_rules:
  - keyword: "press"
    exclude: ["leg press", "calf"]
    level1: upper
    level2: push
    level3: chest
    
  - keyword: "curl"
    level1: upper
    level2: pull
    level3: biceps
    
  - keyword: "squat"
    level1: lower
    level2: legs
    level3: quads
    
  - keyword: "deadlift"
    level1: lower
    level2: legs
    level3: hamstrings
    
  - keyword: "row"
    level1: upper
    level2: pull
    level3: back
    
  - keyword: "raise"
    level1: upper
    level2: push
    level3: shoulders
    
  - keyword: "pull"
    level1: upper
    level2: pull
    level3: back
    
  - keyword: "extension"
    exclude: ["leg extension"]
    level1: upper
    level2: push
    level3: triceps

# Default for completely unknown exercises
default_mapping:
  level1: upper
  level2: push
  level3: unknown
